{
  "name": "war3keys",
  "version": "1.0.0",
  "description": "Warcraft 3 Hotkeys Manager",
  "main": "app/main/main.js",
  "scripts": {
    "dev": "electron-webpack dev",
    "build": "electron-webpack",
    "dist": "run-s build electron-builder",
    "dist:dir": "npm run dist --dir -c.compression=store -c.mac.identity=null",
    "test:jest": "jest",
    "lint": "tslint app/**/*.ts{,x}"
  },
  "author": "drdgvhbh",
  "license": "MIT",
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "app/renderer/**",
      "!app/renderer/index.tsx",
      "!app/renderer/tests/**"
    ],
    "coverageDirectory": ".coverage",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    }
  },
  "electronWebpack": {
    "title": "War3Keys",
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": "src/renderer"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0-beta.55",
    "@types/app-root-path": "^1.2.4",
    "@types/electron-debug": "^1.1.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-is-dev": "^0.3.0",
    "@types/graphql": "^0.13.3",
    "@types/history": "^4.7.0",
    "@types/jest": "^23.3.0",
    "@types/lodash": "^4.14.115",
    "@types/prop-types": "^15.5.4",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.5",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "electron": "^2.0.5",
    "electron-builder": "^20.22.0",
    "electron-webpack": "^2.1.2",
    "electron-webpack-ts": "^2.0.2",
    "jest": "^23.4.1",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "react-test-renderer": "^16.4.1",
    "sass-loader": "^7.0.3",
    "spectron": "^3.8.0",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.55",
    "@material-ui/core": "^1.4.2",
    "@material-ui/icons": "^2.0.0",
    "apollo-client": "^2.3.7",
    "aws-amplify": "^1.0.4",
    "aws-amplify-react": "^1.0.4",
    "aws-appsync": "^1.3.2",
    "aws-appsync-react": "^1.1.2",
    "connected-react-router": "^4.3.0",
    "dotenv": "^6.0.0",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^0.3.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "lodash": "^4.17.10",
    "material-ui-image": "^3.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1",
    "react-image": "^1.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "source-map-loader": "^0.2.3",
    "warcraft3-unit-data": "^1.0.8"
  },
  "files": [
    "*.js",
    "*.ts",
    "dist",
    "node_modules"
  ]
}
